{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze-image.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "api/analyze-image",
      "methods": [
        "POST"
      ],
      "dest": "api/analyze_image.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}