{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze-image.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/analyze-image",
      "methods": [
        "POST"
      ],
      "dest": "api/analyze_image.py"
    }
  ],
  "env": {
    "VERCEL_USE_LEGACY_IMAGE": "1",
    "NODE_VERSION": "18.x"
  }
}